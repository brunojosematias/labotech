{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/no-extraneous-dependencies": "off",
    "import/no-import-module-exports": "off",
    "arrow-body-style": "off",
    "import/prefer-default-export": "off",
    "import/named": "off",
    "import/extensions": "off",
    "max-len": "off",
    "class-methods-use-this": "off",
    "consistent-return": "off",
    "prefer-const": "off"
  }
}
